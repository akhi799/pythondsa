class Solution:
    def flipAndInvertImage(self, image: List[List[int]]) -> List[List[int]]:
        for i in range(len(image)):
            # Flip the row by reversing it
            image[i] = image[i][::-1]
            
            # Invert the image
            for j in range(len(image[0])):
                image[i][j] = 1 - image[i][j]
                
        return image
